{
	"name": "@stella2211/cf-account-switcher",
	"version": "1.0.0",
	"description": "Cloudflare account switcher for wrangler - easily switch between multiple Cloudflare accounts",
	"type": "module",
	"bin": "./src/index.ts",
	"scripts": {
		"start": "bun run src/index.ts",
		"build": "tsgo -p tsconfig.build.json",
		"prepare": "bun run build",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"typecheck": "tsgo -p tsconfig.json"
	},
	"keywords": [
		"cloudflare",
		"wrangler",
		"account",
		"switcher",
		"cli"
	],
	"author": "stella2211",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"@types/bun": "latest",
		"@types/node": "^22.0.0",
		"@typescript/native-preview": "^7.0.0-dev.20251223.1"
	},
	"files": [
		"src",
		"dist"
	],
	"engines": {
		"node": ">=18.0.0"
	}
}
